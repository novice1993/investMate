# 개발 환경 설정

# 뉴스 수집 Cron Job 비활성화 (로컬에서 Render와 중복 실행 방지)
ENABLE_NEWS_COLLECTION_CRON=false

# KOSPI Corp Mapping 동기화 Cron Job 비활성화 (로컬에서 Render와 중복 실행 방지)
ENABLE_KOSPI_MAPPING_SYNC_CRON=false

# 재무 지표 수집 Cron Job 비활성화 (로컬에서 Render와 중복 실행 방지)
ENABLE_FINANCIAL_METRICS_CRON=false

# 일봉 데이터 수집 Cron Job 비활성화 (로컬에서 Render와 중복 실행 방지)
ENABLE_DAILY_PRICES_CRON=false

# 주식 밸류에이션 수집 Cron Job 비활성화 (로컬에서 Render와 중복 실행 방지)
ENABLE_STOCK_VALUATION_CRON=false

# 시그널 스크리닝 Cron Job 비활성화 (로컬에서 Render와 중복 실행 방지)
ENABLE_SIGNAL_SCREENING_CRON=false

# Mock 시그널 활성화 (개발 환경에서 테스트용 시그널 발생 + 조회 시 포함)
MOCK_SIGNAL_ENABLED=false

# Mock KIS WebSocket 활성화 (휴장일에도 실시간 체결가 테스트 가능)
# npm run mock:kis로 Mock 서버를 먼저 실행해야 함
MOCK_KIS_WS_ENABLED=true
MOCK_KIS_WS_PORT=21001
